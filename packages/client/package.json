{
  "name": "@graphix/client",
  "version": "0.1.0",
  "description": "TypeScript API client for Graphix - generated from OpenAPI spec",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./types": {
      "import": "./src/types.ts",
      "types": "./src/types.ts"
    }
  },
  "scripts": {
    "generate": "openapi-typescript openapi-spec.json -o src/schema.d.ts",
    "generate:live": "openapi-typescript http://localhost:3000/api/docs/spec.json -o src/schema.d.ts",
    "build": "tsc --noEmit",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "openapi-fetch": "^0.13.5"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "openapi-typescript": "^7.6.1",
    "typescript": "^5.7.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
